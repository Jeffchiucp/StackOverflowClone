<style>
    .users-container{
        display: inline-grid;
        width: 100%;
        height: 100%;
        grid-template-columns: 1fr 1fr 1fr 1fr 1fr;
        grid-auto-flow: row;
    }

    .userbox{
        height: 100px;
        margin: 10px;
        padding: 5px;
        color: black;

    }

    .userbox:hover{
        background-color: #EEE;
    }

</style>

<div id="users-nav">
    <h2 id="home-header">Users page</h2>
</div>


<div class="users-container">
{{#each userProfile}}
    <a href="byuser?q={{this._id}}">
        <div class="userbox">
            <p>{{this.username}}</p>
        </div>
    </a>
{{/each}}
</div>
